{
    "extends": ["airbnb", "plugin:jasmine/recommended"],
    "plugins": ["jasmine"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] } ],
        "no-console": "off",
        "no-loop-func": "off"
    },
    "env": {
        "browser": true,
        "jasmine": true
    }
}

